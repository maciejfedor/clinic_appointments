<%= search_form_for @q, method: :get, data: { turbo_frame: "patients", turbo_action: "advance", 
 controller: "search", action: "input->search#submit" } do |f| %>
  <%= f.search_field :last_name_start, placeholder: "Last name", class: "shadow appearance-none border rounded py-2 px-3 text-gray-700"%>
   <%= f.select :gender_eq, options_for_select(["Male", "Female"], @q.gender_eq), include_blank: "All" %>
<% end %>